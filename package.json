{
  "name": "vanilla",
  "version": "1.0.0",
  "description": "Vanilla javascript project setup",
  "main": "src/index.js",
  "scripts": {
    "webpack": "webpack --config config/webpack.config.js",
    "build:dev": "npm run webpack -- --env.mode development",
    "build:dev:watch": "npm run webpack -- --env.mode development --watch",
    "start": "webpack-dev-server --config config/webpack.config.js --env.mode development --open",
    "build:prod": "npm run webpack -- --env.mode production",
    "build:prod:inspect": "node --inspect --inspect-brk ./node_modules/.bin/webpack --config config/webpack.config.js --env.mode production",
    "stats": "webpack --config config/webpack.config.js --env.mode production --progress --profile --json > stats.json",
    "checkstats:wba": "webpack-bundle-analyzer stats.json",
    "checkstats:sme": "source-map-explorer dist/*.js",
    "checkstats:bs": "bundle-stats --baseline stats.json",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint \"src/**/*.js\" --quiet",
    "test": "jest",
    "create:docs": "jsdoc -c jsdoc.config.json"
  },
  "author": "Pushkar DK <pushkar100@gmail.com> (http://www.pushkardk.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/plugin-proposal-class-properties": "7.10.1",
    "@babel/plugin-proposal-export-namespace-from": "7.10.1",
    "@babel/plugin-proposal-throw-expressions": "7.10.1",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "bundle-stats": "2.2.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "3.5.3",
    "eslint": "7.2.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsdoc": "27.0.4",
    "eslint-plugin-prettier": "3.1.3",
    "file-loader": "6.0.0",
    "html-webpack-plugin": "4.3.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "jsdoc": "3.6.4",
    "lint-staged": "10.2.9",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "prettier": "2.0.5",
    "source-map-explorer": "2.4.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "3.0.3",
    "url-loader": "4.1.0",
    "webpack": "4.43.0",
    "webpack-bundle-analyzer": "3.8.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "eslint-plugin-jest": "23.13.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format && npm run lint"
    }
  }
}
